        <Label x:Name="label" Content="Datum:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Label x:Name="label1" Content="Potrosnja:" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="31" Margin="114,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="157" FontSize="16"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="31" Margin="114,46,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="157" FontSize="16"/>
        <Button x:Name="button" Content="Dodaj" HorizontalAlignment="Left" Height="40" Margin="10,92,0,0" VerticalAlignment="Top" Width="79" Click="Button_Click"/>
        <Button x:Name="button1" Content="View" HorizontalAlignment="Left" Height="40" Margin="186,92,0,0" VerticalAlignment="Top" Width="85" Click="Button1_Click"/>

    </Grid>
